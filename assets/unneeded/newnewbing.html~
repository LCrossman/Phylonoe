    <script>
	$(function fly() { // wait for document ready
		var flightpath = {
			entry : {
				curviness: 0.25,
				autoRotate: true,
				path: [
						{x: 10,	y: -20},
						{x: 150,y: 0}
					]},
			looping : {
				curviness: 1.25,
				autoRotate: true,
				path: [
						{x: 255,	y: 0},
						{x: 320,	y: -20},
						{x: 400,	y: -40},
						{x: 280,	y: 20},
						{x: 400,	y: 50},
						{x: 480,	y: 20},
						{x: 520,	y: 15}
					]},
			leave : {
				curviness: 0.25,
				autoRotate: true,
				path: [
						{x: 560,	y: 20},
						{x: 600,	y: 40},
						{x: $(window).width() + 300,	y: 0},
					]}};
		// init controller
		// init controller
		var controller = new ScrollMagic.Controller();

		// create tween
	    var tween = gsap.timeline()
			.add(gsap.to($("#plane"), 1.2, {css:{motionPath:flightpath.entry}, ease:Power1.inOut}))
			.add(gsap.to($("#plane"), 2, {css:{motionPath:flightpath.looping}, ease:Power1.inOut}))
			.add(gsap.to($("#plane"), 1, {css:{motionPath:flightpath.leave}, ease:Power1.inOut}));

		// build scene
		var scene = new ScrollMagic.Scene({triggerElement: "#trigger", duration: 800, offset: 20})
	                                        .setPin("#target", {pushFollowers: false})
						.setTween(tween)
		                                .addTo(controller);
	})
    </script>
    
