#!/usr/bin/python

i = 0
keeps = []

while i+1 < len(intervals):
    if intervals[i+1][1] > intervals[i][1] and intervals[i+1][0] < intervals[i][1]:
        newinterv = [intervals[i][0],intervals[i+1][1]]
        keeps.append(newinterv)
        i+=1
    else:
        keeps.append(intervals[i])
    i+=1


print(keeps)
